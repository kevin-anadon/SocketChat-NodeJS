{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2019
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-unused-vars": ["warn"],
      "no-undef": ["error"]
    }
}
